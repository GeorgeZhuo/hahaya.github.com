---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>hahaya</title>
 <link href="http://hahaya.github.com/atom.xml" rel="self"/>
 <link href="http://hahaya.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://hahaya.github.com/</id>
 <author>
   <name>hahaya</name>
   <email>hahayacoder@gmail.com</email>
 </author>
 
 {% for post in site.posts limit:15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://hahaya.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://hahaya.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
